{
  "name": "jssystem",
  "version": "0.1.3",
  "description": "JavaScript System Namespace",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/index.mjs",
  "scripts": {
    "test": "npm run test:build && npm run test:run",
    "prepublish": "npm run build && npm run declaration",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "npx build-esbuild -s src -o build -f cjs",
    "build:esm": "npx build-esbuild -s src -o build -f esm -d false",
    "declaration": "npx tsc --project tsconfig.json",
    "test:build": "npx build-esbuild -s test -o testBuild -f esm",
    "test:run": "node testBuild/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MintedKitten/JSSystem.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "OOP"
  ],
  "author": "MintedKitten",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/object-hash": "^3.0.2",
    "@types/uuid": "^9.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "files": [
    "./build"
  ],
  "dependencies": {
    "big-integer": "^1.6.51",
    "uuid": "^9.0.0"
  }
}
