{
  "name": "jssystem",
  "version": "1.0.0",
  "description": "JavaScript System Namespace",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.mjs",
  "scripts": {
    "test": "jest",
    "prepublish": "npm run bundle && npm run declaration",
    "bundle": "npm run bundle:esm && npm run bundle:cjs",
    "bundle:esm": "esbuild src/index.ts --bundle --format=esm --outfile=build/index.mjs --platform=node --external:./node_modules/* --target=ESNext --minify --allow-overwrite",
    "bundle:cjs": "esbuild src/index.ts --bundle --outfile=build/index.js --platform=node --external:./node_modules/* --target=ESNext --minify --allow-overwrite",
    "declaration": "npx tsc --project tsconfig.json",
    "ts-node:fallback": "npm run fallback:bundle && npm run fallback:run && npm run fallback:cleanup",
    "fallback:bundle": "esbuild test/test.ts --bundle --outfile=test/test.js --platform=node --external:./node_modules/* --target=ESNext --allow-overwrite",
    "fallback:run": "node test/test.js",
    "fallback:cleanup": "npm run cleanup:linux || npm run cleanup:windows",
    "cleanup:linux": "rm -f test/test.js",
    "cleanup:windows": "del \".\\test\\test.js\"",
    "build:testBuild": "npx tsc test/test.ts --outDir testBuild --esModuleInterop --target esnext --moduleResolution node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MintedKitten/JSSystem.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "System"
  ],
  "author": "MintedKitten",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "esbuild": "^0.16.17",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "files": [
    "./build"
  ],
  "dependencies": {
    "big-integer": "^1.6.51"
  }
}
